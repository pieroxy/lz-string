{
    "compilerOptions": {
        "declaration": true,
        "lib": ["ESNext"],
        "module": "ESNext",
        "moduleResolution": "Node",
        "noEmit": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "outDir": "dist",
        "skipLibCheck": true,
        "sourceMap": true,
        "strict": true,
        "target": "ESNext"
    },
    "include": ["src"]
}
