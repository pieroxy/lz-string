{
  "name": "lz-string",
  "version": "1.4.4",
  "license": "MIT",
  "filename": "libs/lz-string.js",
  "description": "LZ-based compression algorithm",
  "homepage": "http://pieroxy.net/blog/pages/lz-string/index.html",
  "keywords": [
    "lz",
    "compression",
    "string"
  ],
  "main": "libs/lz-string.js",
  "typings": "libs/lz-string.d.ts",
  "bin": {
    "lz-string": "bin/bin.js"
  },
  "scripts": {
    "afterBuildForMac": "sed -i '' 's/exports.__esModule = true;/var exports = {};exports.__esModule = true;/g' libs/lz-string.js",
    "afterBuild": "sed -i 's/exports.__esModule = true;/var exports = {};exports.__esModule = true;/g' libs/lz-string.js",
    "build": "npx tsc && npm run afterBuild && npm run minify",
    "test": "npm run tslint && npx jest",
    "tslint": "npx tslint --project .",
    "minify": "jsmin -o libs/lz-string.min.js libs/lz-string.js"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^23.3.9",
    "jest": "^23.6.0",
    "jsmin": "^1.0.1",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "typescript": "^3.1.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pieroxy/lz-string.git"
  },
  "bugs": {
    "url": "https://github.com/pieroxy/lz-string/issues"
  },
  "directories": {
    "test": "tests"
  },
  "author": "pieroxy <pieroxy@pieroxy.net>",
  "autoupdate": {
    "source": "git",
    "target": "git://github.com/pieroxy/lz-string.git",
    "basePath": "libs/",
    "files": [
      "lz-string.js",
      "lz-string.min.js",
      "base64-string.js"
    ]
  }
}
